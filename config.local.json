{
  "app": {
    "name": "next-oms",
    "port": "8080",
    "metricsPort": "9080",
    "sort": "created_at desc",
    "defaultPageSize" : 10,
    "logLevel":   "Info"
  },
  "db": {
    "mysql": {
      "host": "nextOms_mysql",
      "port": "3309",
      "user": "root",
